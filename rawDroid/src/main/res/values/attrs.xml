<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckableRelativeLayout">
        <!-- Defines the drawable to draw over the content.
             This can be used as an overlay. -->
        <attr name="foreground" format="reference|color"/>
    </declare-styleable>
    <declare-styleable name="XmpLabelGroup">
        <!-- Defines whether multiple buttons can be selected simultaneously -->
        <attr name="multiselect" format="boolean"/>
    </declare-styleable>
</resources>