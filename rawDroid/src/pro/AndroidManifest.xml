        </provider>

        <provider
            android:name="com.anthonymandra.content.KeywordProvider"
            android:authorities="com.anthonymandra.rawdroid.KeywordProvider.pro"
            android:grantUriPermissions="true"
            android:exported="false">
        </provider>

    </application>
</manifest>