<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckableRelativeLayout">
        <!-- Defines the drawable to draw over the content.
             This can be used as an overlay. -->
        <attr name="foreground" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="MetaStyle">
        <attr name="metaTextSize" format="dimension" />
        <attr name="metaLabelLength" format="dimension" />
        <attr name="metaValueLength" format="dimension" />
        <attr name="histogramHeight" format="dimension" />
    </declare-styleable>

</resources>